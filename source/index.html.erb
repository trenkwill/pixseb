<div class="modal fade " id="login">
    <div class="modal-dialog index__modal">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close btn btn-default" data-dismiss="modal" aria-hidden="true">&times;</button>

          <h4 class="modal-title text-center">Connect to Pixseb</h4>
        </div>
        <div class="modal-body">
          <div class="index__alert clearfix mb-">
            <p class="small-text">Identifiant et/ou mot de passe invalide</p>
            <a href="#" class="small-text">Oublié ?</a>
          </div>
          <form action="home-initial.html" class="index__form">
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Indentifiant</label>
              <input type="text" name="id" id="id" class="form-control">
            </div>
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Current password</label>
              <input type="password" name="current" id="current" class="form-control">
            </div>
            <div class="text-center mt">
              <input type="submit" value="Login" class="btn btn-lg btn-round bg-blue color-white p- pl+ pr+ mb mt">
              <div>
                <label>
                  <input type="checkbox" id="inlineCheckbox1" value="option1"> Remember me
                </label>
              </div>
              
            </div>
          </form>
        </div>
        <div class="modal-footer">
          <div class="text-center mt">
            <a href="home-initial.html" value="Login" class="btn btn-lg btn-round btn-outline-blue color-blue p- pl+ pr+ mb mt">Sign up</a>
          </div>
        </div>
      </div>
    </div>
  </div>


  <div class="modal fade " id="signup">
    <div class="modal-dialog index__modal">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close btn btn-default" data-dismiss="modal" aria-hidden="true">&times;</button>

          <h4 class="modal-title text-center">Signup to Pixseb</h4>
        </div>
        <div class="modal-body">
          <ul class="p">
            <li class="font-14 mb-">Bénéfice : lorem ipsum dolor sit amet</li>
            <li class="font-14 mb-">Bénéfice : lorem ipsum dolor sit amet</li>
            <li class="font-14 mb-">Bénéfice : lorem ipsum dolor sit amet</li>
          </ul>
          <!-- Alert is hidden (display none) -->
          <div class="index__alert clearfix mb-">
            <p class="small-text">Identifiant et/ou mot de passe invalide</p>
            <a href="#" class="small-text">Oublié ?</a>
          </div>
          <form action="home-initial.html" class="index__form">
            <div class="input-wrap js-float-label-wrapper">
              <label for="">First name</label>
              <input type="text" name="firstname" id="firstname" class="form-control">
            </div>
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Last name</label>
              <input type="text" name="lastname" id="lastname" class="form-control">
            </div>
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Email</label>
              <input type="email" name="current" id="current" class="form-control">
            </div>
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Password</label>
              <input type="password" name="current" id="current" class="form-control">
            </div>
            <div class="input-wrap js-float-label-wrapper">
              <label for="">Confim Password</label>
              <input type="password" name="current" id="current" class="form-control">
            </div>
            <div class="text-center mt">
              <div>
                <label>
                  <input type="checkbox" id="inlineCheckbox1" value="option1"> I accept terms &amp; conditions
                </label>
              </div>
              <input type="submit" value="Sign up for free" class="btn btn-lg btn-round bg-blue color-white p- pl+ pr+ mb mt">
              
            </div>
          </form>
        </div>
        <div class="modal-footer">
          <div class="text-center mt">
            <a href="home-initial.html" value="Login" class="btn btn-lg btn-round btn-outline-blue color-blue p- pl+ pr+ mb">Login</a>
           
          </div>
        </div>
      </div>
    </div>
  </div>

  <%= partial "partials/javascripts" %>

  <script>
  	$( '.js-float-label-wrapper' ).FloatLabel();

  	$(document).on('click', 'a', function(event){
  	    event.preventDefault();

  	    $('html, body').animate({
  	        scrollTop: $( $.attr(this, 'href') ).offset().top
  	    }, 500);
  	});
  </script>
